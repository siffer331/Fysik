[Circumference]
#basic geometri
o=r*2*PI
r=o/2*PI

[ColumbsLaw]
#electric fields
F_E=(1/4*PI*e_0)*Q_1*Q_2/r^2
Q_1=r^2*F_E/(1/4*PI*e_0)*Q_2
Q_2=r^2*F_E/(1/4*PI*e_0)*Q_1
r=sqrt((1/4*PI*e_0)*Q_1*Q_2/F_E)

[ElectricFieldStrength]
#electric fields
E=F_E/Q
Q=F_E/E
F_E=E*Q

[UniformElectricFieldStrength]
#electric fields
E=U/d
U=E*d
d=U/E

[Capacity]
#electric fields
Q=C*U
C=Q/U
U=Q/C

[CapacitorEnergi]
#electric fields
E=Q*U/2
Q=E*2/U
U=E*2/Q

[PlateCapacitorCapacity]
#electric fields
C=e_0*A/d
d=e_0*A/C
A=C*d/e_0

[OhmsLaw]
#electric fields
U=R*I
R=U/I
I=U/R


[TimeConstant]
#electric fields
τ=R*C
R=τ/C
C=τ/R
