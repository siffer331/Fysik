[ColumbsLaw]
F_E = (1/4*π*ε_0)*Q_1*Q_2/d^2
Q_1 =d^2*F_E/(1/4*π*ε_0)*Q_2 
Q_2 =d^2*F_E/(1/4*π*ε_0)*Q_1 
d = sqrt((1/4*π*ε_0)*Q_1*Q_2/F_E)

[ElectricFieldStrength]
#$electric fields
E = F_E/Q 
Q = F_E/E 
F_E = E*Q 

[UniformElectricFieldStrength]
#$electric fields,$uniform field
E = U/d 
U = E*d 
d = U/E 

[Capacity]
#$capacitor
Q = C*U 
C = Q/U 
U = Q/C 

[CapacitorEnergi]
#$capacitor
E = Q*U/2
Q = E*2/U 
U = E*2/Q 

[PlateCapacitorCapacity]
#$capacitor,$plate capacitor
$ε_r = 1
C = ε_0*ε_r*A/d 
d = ε_0*ε_r*A/C 
A = C*d/ε_0*ε_r 
ε_r = C*d/A*ε_0

[OhmsLaw]
U = R*I 
R = U/I 
I = U/R 

[EletricEffect]
P = U*I
U = P/I
I = P/U


[TimeConstant]
τ = R*C 
R = τ/C 
C = τ/R 

[ElectricStrength]
Q = I*Δt
I = Q/Δτ
Δt = Q/I






